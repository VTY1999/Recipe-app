 <table class="table table-striped table-hover mt-5">
  <thead>
    <tr>
     <th scope="col">#</th>
      <th scope="col">Food</th>
      <th scope="col">Quantity</th>
      <th scope="col">Price</th>
    </tr>
  </thead>

  <tbody>
   <% @foods.each do |food| %>
   <% if food.user_id == current_user.id %>
      <tr>
        <td> <%= food.name %></td>
    <%end%> 
      </tr>
      <%end%>
  </tbody>
  </table>